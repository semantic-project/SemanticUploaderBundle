{
    "name": "semantic-project/uploader-bundle",
    "type": "symfony-bundle",
    "description": "A simple symfony3 bundle that attempts to ease file upload",
    "version": "1.0.0",
    "keywords": ["File uploads","upload"],
    "homepage": "https://github.com/semantic-project/SemanticUploaderBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Semantic Project",
            "email": "semantic-project@gmail.com"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": "^7.1",
        "doctrine/annotations": "^1.6",
        "symfony/config": "^3.4|^4.0",
        "symfony/dependency-injection": "^3.4|^4.0",
        "symfony/event-dispatcher": "^3.4|^4.0",
        "symfony/form": "^3.4|^4.0",
        "symfony/http-foundation": "^3.4|^4.0",
        "symfony/http-kernel": "^3.4|^4.0",
        "symfony/templating": "^3.4|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5"
    },
    "suggest": {
        
    },
    "autoload": {
        "psr-4": {
            "Semantic\\UploaderBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Semantic\\TestBundle\\": ""
        }
    },
    "config": {
        "sort-packages": true
    }
}